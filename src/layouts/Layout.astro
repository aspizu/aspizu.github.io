---
import Analytics from '@vercel/analytics/astro'

interface Props {
    title: string
}

const {title} = Astro.props
---

<!doctype html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>{title}</title>
        <link rel="stylesheet" href="/assets/global.css" />
    </head>
    <body>
        <slot />
        <Analytics />
    </body>
</html>
